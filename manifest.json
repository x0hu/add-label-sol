{
  "manifest_version": 3,
  "name": "SOLANA ADD Tag",
  "description": "Address Tagger Extension, for dexscreener/photon/solscan",
  "version": "0.6",
  "icons": {
    "16": "icons/address_tagger_16.png",
    "32": "icons/address_tagger_32.png",
    "48": "icons/address_tagger_48.png",
    "128": "icons/address_tagger_128.png"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "icons/address_tagger_48.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["scripts/content_solana.js"],
      "matches": ["https://solscan.io/*"]
    },
    {
      "js": ["scripts/content.js", "scripts/content2.js", "scripts/content3.js"],
      "matches": ["https://dexscreener.com/*", "https://photon-sol.tinyastro.io/*"]
    }
  ]
}
